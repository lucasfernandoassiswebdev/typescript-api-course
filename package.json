{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API desenvolvida utilizando Typescript, Node, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "disted": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "disted-test": "cross-env NODE_ENV=test node ./dist/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "keywords": [
    "API",
    "Typescript",
    "Node",
    "ES6"
  ],
  "author": "Lucas Fernando de Assis",
  "license": "ISC",
  "dependencies": {
    "@types/typescript": "^2.0.0",
    "bcrypt": "^3.0.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.17.1",
    "cross-env": "^5.2.0",
    "express": "^4.15.2",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.11",
    "morgan": "^1.8.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^6.1.5",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.5",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.34",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "sequelize": "^4.42.0",
    "supertest": "^3.4.2",
    "testdouble": "^3.9.3",
    "ts-node": "^3.0.2"
  }
}
