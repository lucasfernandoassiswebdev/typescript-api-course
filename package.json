{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API desenvolvida utilizando Typescript, Node, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env NODE_ENV=development node ./build/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "Typescript",
    "Node",
    "ES6"
  ],
  "author": "Lucas Fernando de Assis",
  "license": "ISC",
  "dependencies": {
    "@types/typescript": "^2.0.0",
    "body-parser": "^1.17.1",
    "cross-env": "^5.2.0", 
    "express": "^4.15.2",
    "http": "0.0.0",
    "morgan": "^1.8.1",
    "pg": "^6.1.5",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.34",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sequelize": "^4.42.0",
    "supertest": "^3.4.2",
    "testdouble": "^3.9.3",
    "ts-node": "^3.0.2"
  }
}
